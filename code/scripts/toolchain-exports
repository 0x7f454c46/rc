export PATH=~/tools/toolchain/arm-v7a15a7v5r2-x86_64-20140701/bin/:$PATH

function tlch-armt()
{
export ARCH=arm
export CROSS_COMPILE=arm-v7a15a7v5r2-linux-gnueabi-
}

function tlch-arm32()
{
export ARCH=arm
export CROSS_COMPILE=arm-none-eabi-
}

function tlch-arm64()
{
export ARCH=arm64
export CROSS_COMPILE=aarch64-linux-gnu-
}

function tlch-x86()
{
export ARCH=x86_64
export CROSS_COMPILE=
}

#tlch-armt
